{
  "name": "node-rest-api-scrapper",
  "version": "0.1.0",
  "description": "A starting point for Node.js express apps with TypeScript to scrape restful endpoints.",
  "repository": {
    "type": "git",
    "url": "https://github.com/vip-git/node-rest-api-scrapper"
  },
  "author": "Vipin Tanna",
  "license": "MIT",
  "scripts": {
    "start": "nodemon",
    "prestart:prod": "npm run build",
    "start:prod": "NODE_ENV=production node dist/server.js",
    "generate:docs": "npx typedoc --out docs/ src/ --mode file --module commonjs --target es6",
    "build": "tsc && npm run tslint",
    "test": "jest --forceExit",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "path": "tspath"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "./node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/src/**/*.spec.(ts|js)",
      "**/src/**/*.e2e.(ts|js)"
    ],
    "testEnvironment": "node"
  },
  "dependencies": {
    "@tsed/common": "^4.20.0",
    "@tsed/core": "^4.20.0",
    "@tsed/swagger": "^4.20.0",
    "ajv": "^6.5.0",
    "bcrypt-nodejs": "^0.0.3",
    "body-parser": "^1.18.3",
    "class-transformer": "^0.1.9",
    "class-validator": "^0.8.5",
    "compression": "^1.7.2",
    "dotenv": "^6.0.0",
    "errorhandler": "^1.5.0",
    "express": "^4.16.3",
    "express-http-proxy": "^1.2.0",
    "jsonwebtoken": "^8.2.2",
    "lodash": "^4.17.10",
    "lusca": "^1.6.0",
    "moment": "^2.22.2",
    "mongoose": "^5.1.3",
    "morgan": "^1.9.0",
    "node-fetch": "^2.1.2",
    "request": "^2.87.0",
    "request-promise": "^4.2.2",
    "ts-log-debug": "^4.0.3",
    "tspath": "^1.2.10",
    "typedoc": "^0.11.1",
    "typegoose": "5.2.1",
    "typescript-param-validator": "^1.1.0"
  },
  "devDependencies": {
    "@types/async": "^2.0.49",
    "@types/bcrypt-nodejs": "0.0.30",
    "@types/body-parser": "^1.17.0",
    "@types/compression": "0.0.33",
    "@types/connect-mongo": "0.0.34",
    "@types/dotenv": "^2.0.20",
    "@types/express": "^4.16.0",
    "@types/express-session": "^1.15.9",
    "@types/jest": "^21.1.10",
    "@types/lodash": "^4.14.100",
    "@types/mongodb": "^2.2.20",
    "@types/mongoose": "^4.7.36",
    "@types/morgan": "^1.7.32",
    "@types/node": "^7.0.65",
    "@types/nodemailer": "^1.3.32",
    "@types/reflect-metadata": "^0.1.0",
    "@types/request": "^2.47.0",
    "@types/request-promise": "^4.1.41",
    "@types/supertest": "^2.0.4",
    "eslint": "^4.19.1",
    "jest": "^21.2.1",
    "node-sass": "^4.9.0",
    "nodemon": "^1.17.5",
    "shelljs": "^0.7.7",
    "supertest": "^2.0.1",
    "ts-jest": "^21.2.4",
    "ts-node": "^3.3.0",
    "tsconfig-paths": "^3.3.2",
    "tslint": "^5.10.0",
    "typescript": "^2.9.1"
  }
}
